    padding: 1px 18px 1px 3px;
    min-width: 6em;
	max-width: 10em;
    color: #242A38;
	background: #f76163;
    margin: 0 0 0 0;
}

QFontComboBox:editable {
    background: white;
}

QFontComboBox:!editable {
	background: #4e596f;
    /*background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                  stop: 0 rgb(51, 51, 51), stop: 0.4 rgb(39, 39, 39),
                                  stop: 0.5 rgb(32,32,32), stop: 1.0 rgb(38,38,38));*/
}

QFontComboBox::drop-down:editable {
	background: #f76163;
	margin-right: 0px;
	padding-left: 10px;
}

QFontComboBox:!editable:on {
	background: blue;
	margin-right: 0px;
	margin-left: 5px;
}

QFontComboBox::drop-down:editable:on {
	background: #4e596f;
	margin-right: 0px;
	margin-left: 5px;
}

QFontComboBox:on { /* shift the text when the popup opens */
    padding-top: 3px;
    padding-left: 4px;
    background-color: #4e596f;
}

QFontComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: center right;
    width: 8px; 
    right: 3px;
	left: 1px;
    border-top-right-radius: 3px; /* same radius as the QComboBox */
    border-bottom-right-radius: 3px;
    background:#4e596f;
}

QFontComboBox::down-arrow {    
	background: url(:/res/text-editor/dropdown-arrow.png);
    height: 15px;
	width: 22px;
	padding-right: 3px;
}

QComboBox::down-arrow:on { /* shift the arrow when popup is open */
	top: 1px;
    left: 1px;
}

